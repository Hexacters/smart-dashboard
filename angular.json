              "src/app/ng1/bower_components/angular-validation/angular-validation.min.js",
              "src/app/ng1/bower_components/angular-validation/angular-validation-rule.min.js",
              "src/app/ng1/bower_components/angular-validation/angular-validation-match.min.js",
              "src/app/ng1/bower_components/angular-validation/creditcard.js",
              "src/app/ng1/bower_components/angular-file-upload/angular-file-upload.min.js",
              "src/app/ng1/bower_components/angular-ui-utils/ui-utils.min.js",
              "src/app/ng1/bower_components/angular-ui-router/angular-ui-router.min.js",
              "src/app/ng1/bower_components/angular-ui-router/ct-ui-router-extras.min.js",
