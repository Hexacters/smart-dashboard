              "src/app/ng1/bower_components/angular-jwt/angular-jwt.js",
              "src/app/ng1/bower_components/angular-google-analytics/angular-google-analytics.js",
              "src/app/ng1/bower_components/angular-ui-select/select.js",
              "src/app/ng1/bower_components/1.3.13/custom/bootstrap.js",
              "src/app/ng1/bower_components/1.3.13/custom/providers.js",
              "src/app/ng1/bower_components/1.3.13/custom/filters.js",
              "src/app/ng1/bower_components/1.3.13/custom/security.js",
              "src/app/ng1/bower_components/1.3.13/custom/components/visionDatatable-directive.js",
              "src/app/ng1/bower_components/1.3.13/custom/components/visionAction-directive.js",
