              "src/app/ng1/bower_components/1.3.13/custom/components/multiSelect-directive.js",
              "src/app/ng1/bower_components/1.3.13/custom/components/loadingContainer-directive.js",
              "src/app/ng1/bower_components/1.3.13/custom/components/verticalScroll-directive.js",
              "src/app/ng1/bower_components/1.3.13/custom/components/imageCropper-directive.js",
              "src/app/ng1/bower_components/1.3.13/custom/components/visionCheckAll-directive.js",
              "src/app/ng1/bower_components/1.3.13/custom/components/setData-directive.js",
              "src/app/ng1/bower_components/1.3.13/custom/components/autofocus-directive.js",
              "src/app/ng1/bower_components/jquery-fileDownload/jquery.fileDownload.js",
              "src/app/ng1/bower_components/angular-calendar/moment.min.js",
              "src/app/ng1/bower_components/angular-datepicker-popup/datepickerPopup-directive.js",
              "src/app/ng1/bower_components/1.3.13/custom/factories.js",
              "src/app/ng1/bower_components/1.3.13/custom/templates.js",
              "src/app/ng1/bower_components/1.3.13/custom/components/auto-login-directive.js",
              "src/app/ng1/bower_components/jcrop/jquery.Jcrop.min.js",
              "src/app/ng1/bower_components/jquery-maskedinput/jquery.maskedinput.min.js",
              "src/app/ng1/bower_components/1.3.13/custom/components/interAppPopOver-directive.js",
              "src/app/ng1/js/commonController.js",
              "src/app/ng1/js/userController.js",
              "src/app/ng1/js/homeController.js",
              "src/app/ng1/js/signOutBadgeController.js",
              "src/app/ng1/js/resourceCenterController.js",
              "src/app/ng1/js/myProfileController.js",
              "src/app/ng1/js/myDocumentController.js",
              "src/app/ng1/js/manageRepController.js",
              "src/app/ng1/js/documentAlertController.js",
              "src/app/ng1/js/accountController.js",
              "src/app/ng1/js/companyProfileController.js",
              "src/app/ng1/js/printBadgeController.js",
              "src/app/ng1/js/requestappointmentController.js",
              "src/app/ng1/js/shareMyCredentialController.js",
              "src/app/ng1/js/badgePhotoController.js",
              "src/app/ng1/js/companyLogoController.js",
              "src/app/ng1/js/inviteRepController.js",
              "src/app/ng1/js/reportsController.js",
              "src/app/ng1/js/registrationController.js",
              "src/app/ng1/js/footerController.js",
              "src/app/ng1/js/analyticsController.js",
              "src/app/ng1/js/cardProfileController.js",
              "src/app/ng1/js/shoppingCartController.js",
              "src/app/ng1/js/addNewRepController.js",
              "src/app/ng1/js/decideLandingController.js",
              "src/app/ng1/js/paymentReceiptController.js",
              "src/app/ng1/js/accountDetailController.js",
              "src/app/ng1/js/cardManagementController.js",
              "src/app/ng1/js/templates.js",
              "src/app/ng1/js/auditService.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "cpm-ui:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "cpm-ui:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "cpm-ui:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "src/styles.scss"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/popper.js/dist/umd/popper.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.min.js"
            ],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "cpm-ui-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "cpm-ui:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "cpm-ui:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "cpm-ui",
  "cli": {
    "analytics": "2d1e38d0-1218-4b1d-a897-6b9ceb0956cc"
  }
}