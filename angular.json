              "src/app/ng1/bower_components/1.3.13/angular-animate/angular-animate.min.js",
              "src/app/ng1/bower_components/1.3.13/angular-translate/angular-translate.min.js",
              "src/app/ng1/bower_components/angular-autocomplete/angucomplete.js",
              "src/app/ng1/bower_components/angular-ng-table/ng-table.min.js",
              "src/app/ng1/bower_components/bootstrap/bootstrap.min.js",
